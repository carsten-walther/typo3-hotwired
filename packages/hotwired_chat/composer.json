{
    "name": "walther/chat",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Walther\\Chat\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/chat": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "chat"
        }
    }
}