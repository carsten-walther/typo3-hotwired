{
    "name": "walther/hotwired",
    "type": "typo3-cms-extension",
    "description": "@see https://github.com/daCyberpunk/websockets",
    "authors": [
        {
            "name": "Carsten Walther",
            "email": "walther.carsten@web.de",
            "homepage": "https://www.carstenwalther.de",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5",
        "ext-posix": "*",
        "cboden/ratchet": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "Walther\\Hotwired\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/hotwired": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "hotwired"
        }
    }
}